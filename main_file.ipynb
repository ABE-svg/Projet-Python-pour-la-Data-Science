    "def create_profile_table(data, var1, var2):\n",
    "    \n",
    "    contingency_table = pd.crosstab(data[var1], data[var2])\n",
    "    profile_table = contingency_table.div(contingency_table.sum(axis=1), axis=0) * 100\n",
    "    return profile_table"
